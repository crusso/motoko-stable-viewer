{
  "canisters": {
    "viewer_backend": {
      "main": "src/viewer_backend/main.mo",
      "type": "motoko"
    },
    "viewer_frontend": {
      "dependencies": ["viewer_backend"],
      "source": ["dist/viewer_frontend"],
      "type": "assets",
      "workspace": "viewer_frontend",
      "frontend": {
        "entrypoint": "dist/viewer_frontend/index.html"
      }
    }
  },
  "defaults": {
    "build": {
      "args": "--package mycore /home/crusso/motoko-core/src",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
